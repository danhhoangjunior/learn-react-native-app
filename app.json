{
  "expo": {
    "name": "learn-react-native-app",
    "slug": "learn-react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "learn-react-native-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danhhoang.learnreactnativeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.danhhoang.learnreactnativeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://learn-react-native-stripe.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.learn-react-native-app",
          "publishableKey": "pk_test_51Pzgj1DUphgxmqJIM402oJWV7fyxrHtIiAhDbdlyqYphOIfCIqFhHJ8GgP9RyyKs6dAuMfueW18gD8239cinVPtf00F5hiCz64"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://learn-react-native-stripe.expo.app"
      },
      "eas": {
        "projectId": "1e239442-cbce-4f8a-8e70-1906b556f674"
      }
    }
  }
}
